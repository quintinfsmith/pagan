<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <item app:state_invalid="true"
        app:state_focused="false"
        app:state_focused_secondary="false"
        android:color="@color/leaf_invalid" />
    <item app:state_invalid="true"
        app:state_focused="true"
        app:state_focused_secondary="false"
        android:color="@color/leaf_invalid_selected" />
    <item app:state_invalid="true"
        app:state_focused="false"
        app:state_focused_secondary="true"
        android:color="@color/leaf_invalid_selected" />
    <item app:state_focused="false"
        app:state_focused_secondary="false"
        app:state_active="true"
        app:state_spill="false"
        android:color="@color/leaf_main" />
    <item app:state_focused="false"
        app:state_focused_secondary="false"
        app:state_active="false"
        app:state_spill="true"
        android:color="@color/leaf_spill" />
    <item app:state_focused="true"
        app:state_focused_secondary="false"
        app:state_active="false"
        app:state_spill="false"
        android:color="@color/empty_selected" />
    <item app:state_focused="true"
        app:state_focused_secondary="false"
        app:state_active="true"
        app:state_spill="false"
        android:color="@color/leaf_selected" />
    <item app:state_focused="true"
        app:state_focused_secondary="false"
        app:state_spill="true"
        android:color="@color/leaf_selected" />
    <item app:state_focused="false"
        app:state_focused_secondary="true"
        app:state_active="false"
        app:state_spill="false"
        android:color="@color/empty_selected" />
    <item app:state_focused="false"
        app:state_focused_secondary="true"
        app:state_active="true"
        android:color="@color/leaf_secondary" />
    <item app:state_focused="false"
        app:state_focused_secondary="true"
        app:state_spill="true"
        android:color="@color/leaf_secondary" />
    <item app:state_focused="false"
        app:state_focused_secondary="false"
        app:state_active="false"
        app:state_spill="true"
        android:color="@color/leaf_spill" />
    <item app:state_focused="false"
        app:state_focused_secondary="false"
        app:state_active="false"
        app:state_spill="false"
        android:color="@color/channel_even" />
    <!--item android:color="@color/sample_default" /-->

</selector>
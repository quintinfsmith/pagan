<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <item
        app:state_invalid="true"
        android:color="?attr/leaf_bg_invalid" />

    <item
        app:state_invalid="false"
        app:state_focused="true"
        app:state_active="true"
        app:state_linked="false"
        android:color="?attr/leaf_bg_active_focused" />

    <item
        app:state_invalid="false"
        app:state_focused="false"
        app:state_active="true"
        app:state_linked="false"
        android:color="?attr/leaf_bg_active" />

    <item
        app:state_invalid="false"
        app:state_focused="true"
        app:state_active="false"
        app:state_linked="false"
        android:color="?attr/leaf_bg_focused" />
    <item
        app:state_invalid="false"
        app:state_focused="false"
        app:state_active="false"
        app:state_linked="false"
        app:state_channel_even="true"
        android:color="?attr/label_bg_channel_even" />
    <item
        app:state_invalid="false"
        app:state_focused="false"
        app:state_active="false"
        app:state_linked="false"
        app:state_channel_even="false"
        android:color="?attr/label_bg_channel_odd" />

    <item
        app:state_invalid="false"
        app:state_focused="true"
        app:state_active="true"
        app:state_linked="true"
        android:color="?attr/leaf_bg_active_focused_linked" />

    <item
        app:state_invalid="false"
        app:state_focused="false"
        app:state_active="true"
        app:state_linked="true"
        android:color="?attr/leaf_bg_active_linked" />

    <item
        app:state_invalid="false"
        app:state_focused="true"
        app:state_active="false"
        app:state_linked="true"
        android:color="?attr/leaf_bg_focused_linked" />
    <item
        app:state_invalid="false"
        app:state_focused="false"
        app:state_active="false"
        app:state_linked="true"
        android:color="?attr/leaf_bg_linked" />

</selector>

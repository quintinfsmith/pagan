<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <item app:state_invalid="true"
        app:state_focused="false"
        app:state_focused_secondary="false"
        android:color="@color/leaf_invalid_text" />
    <item app:state_invalid="true"
        app:state_focused="true"
        app:state_focused_secondary="false"
        android:color="@color/leaf_invalid_selected_text" />
    <item app:state_invalid="true"
        app:state_focused="false"
        app:state_focused_secondary="true"
        android:color="@color/leaf_invalid_selected_text" />
    <item app:state_focused="false"
        app:state_focused_secondary="false"
        app:state_active="true"
        app:state_spill="false"
        android:color="@color/leaf_text" />
    <item app:state_focused="false"
        app:state_focused_secondary="false"
        app:state_active="false"
        app:state_spill="true"
        android:color="@color/leaf_text" />
    <item app:state_focused="true"
        app:state_focused_secondary="false"
        app:state_active="false"
        app:state_spill="false"
        android:color="@color/empty_selected_text" />
    <item app:state_focused="true"
        app:state_focused_secondary="false"
        app:state_active="true"
        app:state_spill="false"
        android:color="@color/leaf_selected_text" />
    <item app:state_focused="true"
        app:state_focused_secondary="false"
        app:state_spill="true"
        android:color="@color/leaf_selected_text" />
    <item app:state_focused="false"
        app:state_focused_secondary="true"
        app:state_active="false"
        app:state_spill="false"
        android:color="@color/leaf_selected_text" />
    <item app:state_focused="false"
        app:state_focused_secondary="true"
        app:state_active="true"
        android:color="@color/leaf_selected_text" />
    <item app:state_focused="false"
        app:state_focused_secondary="true"
        app:state_spill="true"
        android:color="@color/leaf_selected_text" />
    <item app:state_focused="false"
        app:state_focused_secondary="false"
        app:state_active="false"
        app:state_spill="true"
        android:color="@color/leaf_text" />
    <item app:state_focused="false"
        app:state_focused_secondary="false"
        app:state_active="false"
        app:state_spill="false"
        android:color="@color/leaf_text" />
</selector>
<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <item app:state_focused="true"
        app:state_muted="false"
        android:color="@color/empty_selected" />

    <item app:state_focused="false"
        app:state_muted="false"
        app:state_focused_secondary="false"
        android:color="@color/line_label" />

    <item app:state_focused="false"
        app:state_muted="false"
        app:state_focused_secondary="true"
        android:color="@color/label_selected_secondary" />

    <item app:state_focused="true"
        app:state_muted="true"
        android:color="@color/muted_empty_selected" />

    <item app:state_focused="false"
        app:state_muted="true"
        app:state_focused_secondary="false"
        android:color="@color/muted_channel_even" />

    <item app:state_focused="false"
        app:state_muted="true"
        app:state_focused_secondary="true"
        android:color="@color/muted_label_selected_secondary" />
</selector>
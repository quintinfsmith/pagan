<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item android:elevation="0dp" >
    <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
        <padding
            android:top="-.5dp"
            android:left="-.5dp"
            android:right="0dp"
            android:bottom="0dp" />
    </shape>
</item>
<item android:elevation="0dp" >
    <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
        <stroke android:width="@dimen/stroke_corner" android:color="?attr/table_lines"/>
        <solid android:color="?attr/main_bg"/>
    </shape>
</item>
</layer-list>

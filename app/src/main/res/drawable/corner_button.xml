<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:elevation="0dp" >
        <shape android:shape="rectangle">
            <padding
                android:top="-.5dp"
                android:left="-.5dp"
                android:right="0dp"
                android:bottom="0dp" />
        </shape>
    </item>
    <item android:elevation="0dp" android:id="@+id/tintable_background">
        <shape android:shape="rectangle">
            <solid android:width="@dimen/base_leaf_width" android:color="@color/main_bg"/>
        </shape>
    </item>
    <item android:elevation="0dp" android:id="@+id/tintable_lines">
        <shape android:shape="rectangle">
            <solid android:color="#00000000"/>
            <stroke android:width="@dimen/stroke_corner" android:color="@color/table_lines"/>
        </shape>
    </item>
</layer-list>

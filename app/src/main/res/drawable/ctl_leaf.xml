<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:elevation="0dp" >
        <shape android:shape="rectangle">
            <padding
                android:top="0dp"
                android:left="0dp"
                android:right="0dp"
                android:bottom="0dp" />
        </shape>
    </item>
    <item android:elevation="0dp" android:id="@+id/leaf_background">
        <shape android:shape="rectangle">
            <solid android:color="@color/ctl_leaf_selector"/>
        </shape>
    </item>
    <item android:elevation="0dp" android:id="@+id/tintable_lines">
        <shape android:shape="rectangle">
            <solid android:color="#00000000"/>
            <padding android:bottom="5dp"/>
            <stroke android:width="@dimen/stroke_leaf" android:color="#000000"/>
        </shape>
    </item>

</layer-list>

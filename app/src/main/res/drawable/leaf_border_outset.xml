<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="@dimen/stroke_leaf" android:gravity="start">
        <shape android:shape="rectangle">
            <solid android:color="@color/light_line_bright"/>
        </shape>
    </item>
    <item android:height="@dimen/stroke_leaf" android:start="@dimen/stroke_leaf" android:gravity="top">
        <shape android:shape="rectangle">
            <solid android:color="@color/light_line_bright"/>
        </shape>
    </item>
    <item android:width="@dimen/stroke_leaf" android:end="@dimen/stroke_leaf" android:gravity="end">
        <shape android:shape="rectangle">
            <solid android:color="@color/light_line_dark"/>
        </shape>
    </item>
    <item android:height="@dimen/stroke_leaf" android:bottom="@dimen/stroke_leaf" android:end="@dimen/stroke_leaf" android:gravity="bottom">
        <shape android:shape="rectangle">
            <solid android:color="@color/light_line_dark"/>
        </shape>
    </item>

    <item android:width="@dimen/stroke_leaf" android:end="0dp" android:gravity="end">
        <shape android:shape="rectangle">
            <solid android:color="@color/table_lines"/>
        </shape>
    </item>
    <item android:height="@dimen/stroke_leaf" android:bottom="0dp" android:gravity="bottom">
        <shape android:shape="rectangle">
            <solid android:color="@color/table_lines"/>
        </shape>
    </item>
</layer-list>
<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/tintable_background">
        <shape android:shape="rectangle" >
            <solid android:color="@color/number_select_button_selector"/>
            <corners android:radius="6dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:dashGap="4dp" android:dashWidth="8dp" android:width="3dp" android:color="@color/number_select_button_selector_stroke"/>
            <corners android:radius="4dp"/>
        </shape>
    </item>
</layer-list>

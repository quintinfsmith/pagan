<resources>
    <declare-styleable name="Ranged">
        <attr name="min" format="integer" />
        <attr name="max" format="integer" />
        <attr name="fmin" format="float" />
        <attr name="fmax" format="float" />
        <attr name="precision" format="integer"/>
        <attr name="auto_resize" format="boolean"/>
        <attr name="require_confirm" format="boolean"/>
        <attr name="confirm_on_unfocus" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="NumberSelector">
        <attr name="button_theme" format="integer" />
        <attr name="radix" format="integer" />
        <attr name="entries_per_line" format="integer" />
    </declare-styleable>

    <declare-styleable name="states">
        <attr name="state_active" format="boolean"/>
        <attr name="state_alternate" format="boolean"/>
        <attr name="state_focused" format="boolean" />
        <attr name="state_focused_secondary" format="boolean" />
        <attr name="state_invalid" format="boolean" />
        <attr name="state_channel_even" format="boolean" />
        <attr name="state_spill" format="boolean" />
        <attr name="state_muted" format="boolean" />
    </declare-styleable>

    <attr name="icon_size" format="dimension" />

    <attr name="soundfont_warning_frame" format="color" />
    <attr name="soundfont_warning_background" format="color" />
    <attr name="soundfont_warning_text" format="color" />
    <attr name="soundfont_warning_link" format="color" />

    <declare-styleable name="pagan">
        <attr name="leaf" format="reference"/>
        <attr name="line_label" format="reference"/>
        <attr name="ctl_line_label" format="reference"/>
        <attr name="font_main" format="reference"/>
        <attr name="corner_button" format="reference"/>
    </declare-styleable>
</resources>
